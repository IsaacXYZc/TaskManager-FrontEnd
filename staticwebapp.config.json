{
    "routes": [
      {
        "route": "/{*}",
        "serve": "/index.html",
        "statusCode": 200
      },
      {
        "route": "/{*}.{extension}",
        "serve": "/index.html",
        "statusCode": 200
      }
    ],
    "responseOverrides": {
      "404": {
        "serve": "/index.html",
        "statusCode": 200
      }
    }
  }
  